{
    "AllowedHosts": "*",
    "Aspire": {
        "Npgsql": {
            "EntityFrameworkCore": {
                "PostgreSQL": {
                    "ConnectionString": "skylight-postgress-db",
                    "DisableHealthChecks": false,
                    "DisableMetrics": false,
                    "DisableRetry": false,
                    "DisableTracing": false
                }
            }
        }
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    }
}
