{
  "Jobs": {
    "Storage": {
      "QueuePollInterval": 15,
      "InvisibilityTimeout": 15
    },
    "DeleteHistoricalAlertsJob": {
      "Enabled": true,
      "Schedule": "0 */1 * * *",
      "Threshold": 6
    },
    "PublishDomainEventsJob": {
      "Enabled": true,
      "Schedule": "*/1 * * * *"
    },
    "UpdateCurrentAlertsJob": {
      "Enabled": true,
      "Schedule": "*/5 * * * *"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Hangfire": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Skylight": "Debug"
    }
  }
}
